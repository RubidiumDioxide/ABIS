<Window x:Class="abis_app.ReportsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:abis_app"
        mc:Ignorable="d"
        Title="ReportsWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="Book_Reports_Button" Content="Отчеты по поставкам книг" Style="{StaticResource Button_Style}" Grid.Row="1" Grid.Column="2" Click="Reports_Book_Button_Click"/>
    </StackPanel>

</Window>
